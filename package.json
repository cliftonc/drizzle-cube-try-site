{
  "name": "drizzle-cube-try-site",
  "version": "1.0.0",
  "description": "Source code for https://try.drizzle-cube.dev",
  "type": "module",
  "scripts": {
    "preinstall": "node -e \"if (process.platform === 'linux' && process.arch === 'x64') { console.log('Linux x64 detected - ensuring Rollup binary compatibility'); }\"",
    "dev": "concurrently \"npm:dev:worker\" \"npm:dev:client\"",
    "dev:server": "tsx watch src/index.ts",
    "dev:worker": "wrangler dev --test-scheduled --port 3459 --live-reload",
    "dev:client": "vite",
    "dev:full": "concurrently \"npm:dev:worker\" \"npm:dev:client\"",
    "build": "npm run build:client && npm run build:worker",
    "build:client": "tsc --project tsconfig.client.json && vite build",
    "build:worker": "tsc --project tsconfig.worker.json",
    "build:full": "npm run build",
    "start": "node dist/index.js",
    "deploy": "npm run build && node scripts/inject-analytics.js && wrangler deploy",
    "deploy:staging": "npm run build && node scripts/inject-analytics.js && wrangler deploy --env staging",
    "cf:login": "wrangler auth login",
    "cf:whoami": "wrangler whoami",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "typecheck": "tsc --noEmit",
    "setup": "npm run docker:up && sleep 5 && npm run db:generate && npm run db:migrate && npm run db:seed",
    "setup:neon": "npm run db:generate && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@neondatabase/serverless": "^1.0.1",
    "@tanstack/react-query": "^5.0.0",
    "dotenv": "^17.2.1",
    "drizzle-cube": "^0.1.26",
    "drizzle-orm": "^0.44.4",
    "hono": "^4.9.4",
    "postgres": "^3.4.0",
    "react-grid-layout": "^1.4.0",
    "react-resizable": "^3.0.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250816.0",
    "@heroicons/react": "^2.2.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@types/react-grid-layout": "^1.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.31.4",
    "postcss": "^8.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.24.0",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wrangler": "^4.32.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.0"
  },
  "keywords": [
    "analytics",
    "drizzle",
    "hono",
    "cube",
    "semantic-layer"
  ],
  "author": "Your Name",
  "license": "MIT"
}
